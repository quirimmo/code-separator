{
    "name": "helloworldextension",
    "displayName": "HelloWorldExtension",
    "description": "test extension",
    "version": "0.0.1",
    "publisher": "Quirino Lo Russo",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown",
        "*"
    ],
    "contributes": {
        "commands": [
			{
				"command": "helloworldextension.test",
				"title": "Test Command"
			}
		],
        "configuration": {
            "type": "object",
            "title": "Test configuration",
            "properties": {
                "helloworldextension.objarrconf": {
                    "type": "string",
                    "default": "YES I AM!",
                    "description": "Complete functions with their parameter signature."
                }
            }
        },
        "keybindings": [{
            "command": "helloworldextension.test",
            "key": "ctrl+alt+q",
            "mac": "cmd+alt+q",
            "when": "editorTextFocus"
        }]
    },
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}